<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fernandogodinez.loteria" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Principal_activity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Griton"
            android:label="@string/app_name"
            android:parentActivityName=".Principal_activity"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.fernandogodinez.loteria.Principal_activity" />
        </activity>
        <activity
            android:name=".play"
            android:label="@string/title_activity_play"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Seleccionar_objetivo"
            android:label="@string/title_activity_seleccionar_objetivo"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Tabla3x3"
            android:label="@string/title_activity_tabla3x3"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Tabla4x4"
            android:label="@string/title_activity_tabla4x4"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Tabla5x5"
            android:label="@string/title_activity_tabla5x5"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Jugadas"
            android:label="@string/title_activity_jugadas"
            android:parentActivityName=".Griton"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.fernandogodinez.loteria.Griton" />
        </activity>
    </application>
</manifest>

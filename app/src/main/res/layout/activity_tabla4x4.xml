<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.fernandogodinez.loteria.Tabla4x4"
    android:background="@drawable/lote_backgroud">

    <TextView android:text="@string/tabla4x4" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton1"
        android:layout_marginTop="91dp"
        android:src="@drawable/ic_launcher256"
        android:layout_below="@+id/text"
        android:layout_alignEnd="@+id/text"
        android:layout_marginEnd="72dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton2"
        android:src="@drawable/ic_launcher256"
        android:layout_alignTop="@+id/imageButton1"
        android:layout_toEndOf="@+id/imageButton5" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton3"
        android:layout_alignTop="@+id/imageButton2"
        android:layout_toEndOf="@+id/imageButton2"
        android:src="@drawable/ic_launcher256" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton4"
        android:src="@drawable/ic_launcher256"
        android:layout_alignTop="@+id/imageButton3"
        android:layout_toEndOf="@+id/imageButton3" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton5"
        android:layout_below="@+id/imageButton1"
        android:layout_alignStart="@+id/imageButton1"
        android:src="@drawable/ic_launcher256" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton6"
        android:src="@drawable/ic_launcher256"
        android:layout_below="@+id/imageButton1"
        android:layout_toStartOf="@+id/imageButton3" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton7"
        android:layout_alignBottom="@+id/imageButton6"
        android:layout_toEndOf="@+id/imageButton6"
        android:src="@drawable/ic_launcher256" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton8"
        android:layout_below="@+id/imageButton3"
        android:layout_toEndOf="@+id/imageButton7"
        android:src="@drawable/ic_launcher256" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton9"
        android:layout_below="@+id/imageButton5"
        android:layout_alignStart="@+id/imageButton5"
        android:src="@drawable/ic_launcher256" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton10"
        android:layout_alignBottom="@+id/imageButton9"
        android:layout_toEndOf="@+id/imageButton9"
        android:src="@drawable/ic_launcher256" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton11"
        android:layout_alignBottom="@+id/imageButton10"
        android:layout_toEndOf="@+id/imageButton10"
        android:src="@drawable/ic_launcher256" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton12"
        android:layout_alignBottom="@+id/imageButton11"
        android:layout_toEndOf="@+id/imageButton11"
        android:src="@drawable/ic_launcher256" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton13"
        android:layout_below="@+id/imageButton9"
        android:layout_alignStart="@+id/imageButton9"
        android:src="@drawable/ic_launcher256" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton14"
        android:layout_alignTop="@+id/imageButton13"
        android:layout_toEndOf="@+id/imageButton13"
        android:src="@drawable/ic_launcher256" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton15"
        android:layout_alignBottom="@+id/imageButton14"
        android:layout_toEndOf="@+id/imageButton14"
        android:src="@drawable/ic_launcher256" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton16"
        android:layout_alignBottom="@+id/imageButton15"
        android:layout_toEndOf="@+id/imageButton15"
        android:src="@drawable/ic_launcher256" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Loteria"
        android:id="@+id/button4loteria"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/imageButton13" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Salir"
        android:id="@+id/button4salir"
        android:layout_alignBottom="@+id/button4loteria"
        android:layout_alignEnd="@+id/imageButton16" />

</RelativeLayout>
